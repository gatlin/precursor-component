{
  "name": "precursor-component",
  "version": "0.1.2",
  "description": "embedded precursor vm in a webcomponent",
  "author": "gatlin@niltag.net",
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "format": "prettier --write \"{index.ts,src/**/*.ts}\"",
    "lint": "eslint \"{index.ts,src/**/*.ts}\" --fix",
    "prepublishOnly": "tsc",
    "example": "tsc -p tsconfig.json && concurrently -k -r \"tsc -p tsconfig.json --watch --preserveWatchOutput\" \"wds --node-resolve --watch\""
  },
  "dependencies": {
    "lit": "^2.0.0-rc.2",
    "lit-robot-manager": "github:gatlin/manager",
    "precursor-ts": "github:gatlin/precursor-ts",
    "robot3": "^0.2.19",
    "torc": "github:gatlin/torc#b3658ab99c73725ee783a2ebe190a0fa8f6851d5",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@web/dev-server": "^0.1.18",
    "@web/dev-server-storybook": "^0.1.3",
    "concurrently": "^5.3.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  }
}
